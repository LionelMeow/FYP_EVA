@model IEnumerable<FYP_EVA.Models.Organiser>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.OrganiserName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OrganiserType)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Password)
